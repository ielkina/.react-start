{
  "name": "react-start",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ielkina.github.io/.react-start/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:js": "eslint src/**/*.{js,jsx}",
    "prepare": "husky install",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "server": "serve -s build -l 4000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "browserslist": "^4.23.3",
    "date-fns": "^2.23.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@types/styled-components": "^5.1.34",
    "clsx": "^2.1.1",
    "eslint": "^8.57.0",
    "modern-normalize": "^3.0.1",
    "prettier": "^3.3.3",
    "styled-components": "^6.1.13"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "json-comments": {
    "tips": "Установите плагин JsonComments, чтобы активировать возможность комментирования для файлов JSON. См.: https://github.com/zhangfisher/json_comments_extension",
    "jsconfig.json": {
      "compilerOptions": "Настройка относительных путе"
    }
  }
}