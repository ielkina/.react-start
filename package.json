{
  "name": "react-start",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://ielkina.github.io/react-start/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:js": "eslint src/**/*.{js,jsx}",
    "dev": "vite",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "server": "serve -s build -l 4000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "browserslist": "^4.23.3",
    "clsx": "^2.1.1",
    "date-fns": "^2.23.0",
    "modern-normalize": "^2.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.26.2",
    "react-scripts": "5.0.1",
    "vite-plugin-svga": "^4.2.0",
    "web-vitals": "^2.1.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.2.1",
    "clsx": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "modern-normalize": "^3.0.1",
    "prettier": "^3.3.3",
    "styled-components": "^6.1.13",
    "vite": "^5.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "prop-types": "^15.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "json-comments": {
    "tips": "Установите плагин JsonComments, чтобы активировать возможность комментирования для файлов JSON. См.: https://github.com/zhangfisher/json_comments_extension",
    ".prettierrc.json": {}
  }
}